    <a href="{{   path('home') }}"> <img src="{{   asset("img/logo.png") }}" style="height:35px;padding-left:10px;"></a>
    <div class="top-bar-left">
        <a href="{{   path('home') }}" id="trademark">   Bruno&nbsp;HUDBERT</a>
    </div>

    <div class="top-bar-right">
        <ul class="menu ">
            <li><a href="#">Blog</a></li>
            <li><a href="#">Formations</a></li>
            <li><a href="#">Portfolio</a></li>
            <li><a href="#" id="last">CV</a></li>

